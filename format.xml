<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <RECORD>
  <FIELD ID="0" xsi:type="CharTerm" TERMINATOR='"' MAX_LENGTH="10"/>
  <FIELD ID="1" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="510"/>
  <FIELD ID="2" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="255"/>
  <FIELD ID="3" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="255"/>
  <FIELD ID="4" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="50"/>
  <FIELD ID="5" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="50"/>
  <FIELD ID="6" xsi:type="CharTerm" TERMINATOR=';\0' MAX_LENGTH="50"/>
